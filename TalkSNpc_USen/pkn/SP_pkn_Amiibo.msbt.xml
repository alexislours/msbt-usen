	<message label="001" attrib="00 00 00 13"><anim id="Laughing" target="actor" targetSpec="" />Hee hee hee! Ho ho ho! <delay frames="30" /><anim id="EmotEnd" target="actor" targetSpec="" />It is I, <delay20 /><anim id="SmugFace" target="actor" targetSpec="" /><colour id="3" />Jack<colour id="65535" />,
