	<message label="101" attrib="00 00 02 00 00 00 00 00"><anim id="Nodding" target="actor" targetSpec="" />Hmm<tag a="110" b="0" /> "<tag a="50" b="3" /><tag a="110" b="5" />," was it?</message>
	<message label="101_a" attrib="00 00 00 00 00 00 00 00">It was!</message>
	<message label="101_b" attrib="00 00 00 00 00 00 00 00">It wasn't.</message>
