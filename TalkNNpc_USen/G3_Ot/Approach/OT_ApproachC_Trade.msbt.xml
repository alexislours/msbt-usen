	<message label="002_b" attrib="00 00 00 00 00 00 00 00">That's not fun!</message>
	<message label="003" attrib="00 00 02 00 00 00 00 00"><anim id="Sighing" target="actor" targetSpec="" />Ugh<tag a="110" b="0" /> Sometimes I am SO careless.<delay40 /><anim id="Worried" target="actor" targetSpec="" />
