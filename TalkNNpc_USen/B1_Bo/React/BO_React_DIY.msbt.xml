	<message label="011" attrib="00 00 00 00 00 00 00 00"><tag a="60" b="20" /></message>
	<message label="017" attrib="00 00 00 00 00 00 00 00"><anim id="BigSmile" target="actor" targetSpec="" />Leave it to me!<delay frames="50" /> <anim id="FaintSmile" target="actor" targetSpec="" />I got it all scribbled
