<msbt attribsize="8">
	<message label="001" attrib="00 00 02 00 00 00 00 00">Hey! Sorry about the mess, but
I ain't got time to clean today<tag a="110" b="0" /></message>
	<message label="001_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />What are you doing?</message>
