	<message label="015" attrib="00 00 02 00 00 00 00 00"><anim id="SmugFace" target="actor" targetSpec="" />Check me out! <anim id="Greeting" target="actor" targetSpec="" />I'm doin' <delay8 />STUFF!</message>
	<message label="015_a" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />What are you doing?</message>
	<message label="015_b" attrib="00 00 00 00 00 00 00 00"><anim id="EmotEnd" target="actor" targetSpec="" />You look busy.</message>
	<message label="016" attrib="00 00 02 00 00 00 00 00"><anim id="Shocked" target="actor" targetSpec="" /><tag a="50" b="3" /><tag a="110" b="3" />!<delay frames="40" />
<anim id="Confused" target="actor" targetSpec="" />Um. Watch your step, OK?</message>
