<anim id="BigSmile" target="actor" targetSpec="" /><tag a="90" b="21" arg="0x0" />!</message>
	<message label="100_a" attrib="00 00 00 00 00 00 00 00">I'll buy it!</message>
	<message label="100_b" attrib="00 00 00 00 00 00 00 00">No, that's OK.</message>
