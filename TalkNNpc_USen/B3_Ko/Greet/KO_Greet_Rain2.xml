	<message label="005" attrib="00 00 0c 01 00 00 00 00"><anim id="Thinking" target="mainNpc" targetSpec="" />"<tag a="10" b="13" /><tag a="50" b="3" /><tag a="110" b="8" /><endTag a="10" b="13" /> in the Rain"<tag a="110" b="0" />
