	<message label="305" attrib="00 00 03 00 00 00 00 00"><anim id="Eh" target="actor" targetSpec="" />So, <tag a="115" b="1" arg="0xcd00" />?</message>
	<message label="305_a" attrib="00 00 00 00 00 00 00 00">Yeah.</message>
