<msbt attribsize="8">
	<message label="011" attrib="00 00 0c 01 00 00 00 07"><anim id="Hello" target="actor" targetSpec="" />Welcome!<delay40 /> <anim id="FaintSmile" target="actor" targetSpec="" />Please make yourself at
